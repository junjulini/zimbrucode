<!DOCTYPE html>
<html {{ body.languageAttributes|raw }}>
    <head>
        {% block startHead %}
            <meta charset="{{ glob.bloginfo('charset')|e('html_attr') }}">
            <meta http-equiv="X-UA-Compatible" content="IE=edge">
            <meta name="viewport" content="width=device-width, initial-scale=1">
            <link rel="profile" href="http://gmpg.org/xfn/11">

            {% block head %}{% endblock %}
        {% endblock %}

        {{ body.head|raw }}

        {% block endHead %}{% endblock %}
    </head>

    <body {{ body.classes|raw }}>
        {{ body.firstAction|raw }}

        {% block body %}{% endblock %}

        {{ body.footer|raw }}
    </body>
</html>